<div style="background-color: white">
    <table class="table table-hover table-bordered">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Thời điểm ra giá</th>
            <th scope="col">Người ra giá</th>
            <th scope="col">Giá hiện tại</th>
        </tr>
        </thead>
        <tbody>
        {{#each this.Users}}
            <tr>
                <th scope="row">{{No}}</th>
                <td>{{Time}}</td>
                <td>{{MaskCharacter Username}}</td>
                <td>{{Price}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
